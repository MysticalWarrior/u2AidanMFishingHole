<Window x:Class="u2AidanMFishingHole.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:u2AidanMFishingHole"
        mc:Ignorable="d"
        Title="Fishing limiter 9001" Height="350" Width="525">
    <ScrollViewer>
    <StackPanel x:Name="myStackPanel">
        <Label x:Name="greeting" Content="Remeber to press enter after the last line."/>
        <TextBox x:Name="txtInput" AcceptsReturn="True" TextWrapping="Wrap" MinHeight="30"/>
        <Button x:Name="btnRun" Click="btnRun_Click" Content="Run" Width="125" HorizontalAlignment="Left"/>
    </StackPanel>
    </ScrollViewer>
</Window>
